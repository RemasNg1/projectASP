
@{
    var product = (List<ProductModel>)ViewData["product"];
}


<table class="table">

    <thead>
        <tr>
         <th>Id</th>
        <th>Name</th>
         <th>Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var p in product)
        {
    
        <tr>
            <td>@p.Id</td>
                <td>@p.Name</td>
                <td>@p.Price</td>
    </tr>

        }
    </tbody>

</table>